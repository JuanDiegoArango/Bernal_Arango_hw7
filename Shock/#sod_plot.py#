import numpy as np
import matplotlib.pyplot as plt
import sys

nombre = sys.argv[1]

nombrepdf = nombre.split(".")

datos = np.loadtxt(nombre)

nombreNum= nombre.split("_")

plt.scatter(datos[:,1],datos[:,2])
plt.xlabel("Numero Presas")
plt.ylabel("Numero Depradores")
plt.title("Numero Presas:"+nombreNum[1]+ "       Numero Depredadores: 20 ")
plt.savefig(nombrepdf[0]+".pdf")